{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js ",
    "start2": "port=4000 node scripts/start.js ",
    "build": "node scripts/build.js",
    "build:dev": "SERVER_ENV=development node scripts/build.js",
    "build:uat": "SERVER_ENV=uat node scripts/build.js",
    "build:prod": "SERVER_ENV=production node scripts/build.js",
    "build:staging": "SERVER_ENV=staging node scripts/build.js",
    "test_0": "node scripts/test.js --env=jsdom ",
    "test": "jest --forceExit --runInBand",
    "jest": "jest --runInBand --watch test ",
    "mock": "nodemon --inspect server/schema/mock.js -w server",
    "node": "NODE_ENV=local port=3001 node server",
    "debug": "NODE_ENV=local port=3001 node --inspect server",
    "debug:socket": "NODE_ENV=development port=6180 node server/socket.js && yarn debug",
    "node:staging": "NODE_ENV=staging node server",
    "node:prod": "NODE_ENV=production node server",
    "node:dev": "NODE_ENV=development port=80 node server",
    "node:uat": "NODE_ENV=uat port=8081 node server",
    "node:win": "set NODE_ENV=local&&set port=3001&&node server",
    "node:windebug": "set NODE_ENV=local&&set port=3001&&node --inspect server",
    "node:mon": "NODE_ENV=local port=3001 nodemon --inspect server -w server -w config --delay 0.5",
    "node:mon2": "NODE_ENV=local port=4001 nodemon --inspect=9231 server -w server -w config --delay 0.5",
    "nodemon": "yarn node:mon",
    "nodemon2": "yarn node:mon2",
    "pm2:prod": "pm2 reload edi --env production",
    "pm2:dev": "pm2 reload edi",
    "pm2:uat": "pm2 reload edi --env uat",
    "reload:prod": "yarn build:prod && yarn pm2:prod",
    "reload:dev": "yarn build:dev && yarn pm2:dev",
    "reload:uat": "yarn build:uat && yarn pm2:uat",
    "react": "git pull && yarn install && yarn build",
    "eslint": "eslint src"
  },
  "dependencies": {
